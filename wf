function isTwentyAndHalfOrAbove(dobString) {
    // Convert DOB from YYYY-MM-DD format to a Date object
    var dob = new Date(dobString);

    // Get today's date
    var today = new Date();
    var ageYears = today.getFullYear() - dob.getFullYear();
    var ageMonths = today.getMonth() - dob.getMonth();
    var ageDays = today.getDate() - dob.getDate();

    // Adjust the age in months and years if necessary
    if (ageMonths < 0 || (ageMonths === 0 && ageDays < 0)) {
        ageYears--;
        ageMonths += 12;
    }

    // Calculate total age in months
    var totalAgeInMonths = ageYears * 12 + ageMonths;

    // Check if age is 20.5 years or above (20 years and 6 months)
    return totalAgeInMonths >= (20 * 12 + 6);
}

// Example usage
var dobString = "2003-07-15"; // Replace with actual DOB in YYYY-MM-DD format
console.log(isTwentyAndHalfOrAbove(dobString)); // Output: true or false
